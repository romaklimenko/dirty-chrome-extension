{
  "name": "d3.ru++",
  "version": "1.0.5",
  "description": "Полезные твики для d3.ru.",
  "permissions": [ "*://d3.ru/user/*" ],
  "content_scripts": [
    {
      "js": [ "common.js" ],
      "matches": [ "*://d3.ru/user/*" ]
    },
    {
      "js": [ "features/bans.js" ],
      "matches": [ "*://d3.ru/user/*" ]
    },
    {
      "js": [ "features/notes.js" ],
      "matches": [ "*://d3.ru/user/*" ]
    },
    {
      "js": [ "features/hotkeys.js" ],
      "matches": [ "*://d3.ru/*", "*://*.d3.ru/*" ]
    }
  ],
  "icons": {
    "16": "images/favicon16.png",
    "32": "images/favicon32.png",
    "48": "images/favicon48.png",
    "128": "images/favicon128.png"
  },
  "manifest_version": 2
}